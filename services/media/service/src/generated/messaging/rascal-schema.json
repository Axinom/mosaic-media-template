{
  "vhosts": {
    "vhost-name": {
      "assert": false,
      "exchanges": {
        "command": {
          "type": "topic"
        },
        "event": {
          "type": "topic"
        },
        "delay": {
          "type": "topic"
        },
        "retry": {
          "type": "topic"
        },
        "dead_letter": {
          "type": "topic"
        }
      },
      "queues": {
        "delay:10sec": {
          "options": {
            "arguments": {
              "x-message-ttl": 10000,
              "x-dead-letter-exchange": "retry"
            }
          }
        },
        "delay:30sec": {
          "options": {
            "arguments": {
              "x-message-ttl": 30000,
              "x-dead-letter-exchange": "retry"
            }
          }
        },
        "media-service:dead_letter": {
          "options": {
            "arguments": {
              "x-queue-type": "quorum"
            }
          }
        },
        "media-service:inbox": {
          "options": {
            "arguments": {
              "x-dead-letter-exchange": "dead_letter",
              "x-dead-letter-routing-key": "media-service.dead_letter",
              "x-queue-type": "quorum"
            }
          }
        },
        "ax-video-service:inbox": {
          "options": {
            "arguments": {
              "x-dead-letter-exchange": "dead_letter",
              "x-dead-letter-routing-key": "ax-video-service.dead_letter",
              "x-queue-type": "quorum"
            }
          }
        },
        "ax-image-service:inbox": {
          "options": {
            "arguments": {
              "x-dead-letter-exchange": "dead_letter",
              "x-dead-letter-routing-key": "ax-image-service.dead_letter",
              "x-queue-type": "quorum"
            }
          }
        }
      },
      "bindings": {
        "media-service:dead_letter": {
          "source": "dead_letter",
          "bindingKey": "media-service.dead_letter",
          "destination": "media-service:dead_letter"
        },
        "delay:10sec": {
          "source": "delay",
          "bindingKey": "delay.10sec",
          "destination": "delay:10sec"
        },
        "delay:30sec": {
          "source": "delay",
          "bindingKey": "delay.30sec",
          "destination": "delay:30sec"
        },
        "retry:media-service:inbox": {
          "source": "retry",
          "bindingKey": "media-service:inbox.#",
          "destination": "media-service:inbox"
        },
        "media-service.ingest.start": {
          "source": "command",
          "bindingKey": "media-service.ingest.start",
          "destination": "media-service:inbox"
        },
        "media-service.ingest.start_item": {
          "source": "command",
          "bindingKey": "media-service.ingest.start_item",
          "destination": "media-service:inbox"
        },
        "media-service.ingest.update_metadata": {
          "source": "command",
          "bindingKey": "media-service.ingest.update_metadata",
          "destination": "media-service:inbox"
        },
        "media-service.ingest.check_finish_item": {
          "source": "command",
          "bindingKey": "media-service.ingest.check_finish_item",
          "destination": "media-service:inbox"
        },
        "media-service.ingest.check_finish_document": {
          "source": "command",
          "bindingKey": "media-service.ingest.check_finish_document",
          "destination": "media-service:inbox"
        },
        "retry:ax-video-service:inbox": {
          "source": "retry",
          "bindingKey": "ax-video-service:inbox.#",
          "destination": "ax-video-service:inbox"
        },
        "ax-video-service.*.*.video.ensure_exists_already_existed": {
          "source": "event",
          "bindingKey": "ax-video-service.*.*.video.ensure_exists_already_existed",
          "destination": "media-service:inbox"
        },
        "ax-video-service.*.*.video.ensure_exists_creation_started": {
          "source": "event",
          "bindingKey": "ax-video-service.*.*.video.ensure_exists_creation_started",
          "destination": "media-service:inbox"
        },
        "ax-video-service.*.*.video.ensure_exists_failed": {
          "source": "event",
          "bindingKey": "ax-video-service.*.*.video.ensure_exists_failed",
          "destination": "media-service:inbox"
        },
        "retry:ax-image-service:inbox": {
          "source": "retry",
          "bindingKey": "ax-image-service:inbox.#",
          "destination": "ax-image-service:inbox"
        },
        "ax-image-service.*.*.image.ensure_exists_already_existed": {
          "source": "event",
          "bindingKey": "ax-image-service.*.*.image.ensure_exists_already_existed",
          "destination": "media-service:inbox"
        },
        "ax-image-service.*.*.image.ensure_exists_created": {
          "source": "event",
          "bindingKey": "ax-image-service.*.*.image.ensure_exists_created",
          "destination": "media-service:inbox"
        },
        "ax-image-service.*.*.image.ensure_exists_failed": {
          "source": "event",
          "bindingKey": "ax-image-service.*.*.image.ensure_exists_failed",
          "destination": "media-service:inbox"
        },
        "media-service.entity.publish_entity": {
          "source": "command",
          "bindingKey": "media-service.entity.publish_entity",
          "destination": "media-service:inbox"
        },
        "media-service.entity.unpublish_entity": {
          "source": "command",
          "bindingKey": "media-service.entity.unpublish_entity",
          "destination": "media-service:inbox"
        },
        "media-service.entity.delete": {
          "source": "command",
          "bindingKey": "media-service.entity.delete",
          "destination": "media-service:inbox"
        },
        "ax-image-service.*.*.image_types.declared": {
          "source": "event",
          "bindingKey": "ax-image-service.*.*.image_types.declared",
          "destination": "media-service:inbox"
        },
        "ax-image-service.*.*.image_types.declare_failed": {
          "source": "event",
          "bindingKey": "ax-image-service.*.*.image_types.declare_failed",
          "destination": "media-service:inbox"
        },
        "ax-video-service.*.*.cue_point_types.declared": {
          "source": "event",
          "bindingKey": "ax-video-service.*.*.cue_point_types.declared",
          "destination": "media-service:inbox"
        },
        "ax-video-service.*.*.cue_point_types.declare_failed": {
          "source": "event",
          "bindingKey": "ax-video-service.*.*.cue_point_types.declare_failed",
          "destination": "media-service:inbox"
        }
      },
      "publications": {
        "retry_in_10sec": {
          "exchange": "delay",
          "options": {
            "CC": [
              "delay.10sec"
            ]
          }
        },
        "retry_in_30sec": {
          "exchange": "delay",
          "options": {
            "CC": [
              "delay.30sec"
            ]
          }
        },
        "StartIngest": {
          "exchange": "command",
          "routingKey": "media-service.ingest.start"
        },
        "StartIngestItem": {
          "exchange": "command",
          "routingKey": "media-service.ingest.start_item"
        },
        "UpdateMetadata": {
          "exchange": "command",
          "routingKey": "media-service.ingest.update_metadata"
        },
        "CheckFinishIngestItem": {
          "exchange": "command",
          "routingKey": "media-service.ingest.check_finish_item"
        },
        "CheckFinishIngestDocument": {
          "exchange": "command",
          "routingKey": "media-service.ingest.check_finish_document"
        },
        "EnsureVideoExists": {
          "exchange": "command",
          "routingKey": "ax-video-service.*.*.video.ensure_exists"
        },
        "EnsureImageExists": {
          "exchange": "command",
          "routingKey": "ax-image-service.*.*.image.ensure_exists"
        },
        "PublishEntity": {
          "exchange": "command",
          "routingKey": "media-service.entity.publish_entity"
        },
        "UnpublishEntity": {
          "exchange": "command",
          "routingKey": "media-service.entity.unpublish_entity"
        },
        "MoviePublished": {
          "exchange": "event",
          "routingKey": "media-service.movie.published"
        },
        "MovieUnpublished": {
          "exchange": "event",
          "routingKey": "media-service.movie.unpublished"
        },
        "MovieGenresPublished": {
          "exchange": "event",
          "routingKey": "media-service.movie-genres.published"
        },
        "MovieGenresUnpublished": {
          "exchange": "event",
          "routingKey": "media-service.movie-genres.unpublished"
        },
        "TvshowGenresPublished": {
          "exchange": "event",
          "routingKey": "media-service.tvshow-genres.published"
        },
        "TvshowGenresUnpublished": {
          "exchange": "event",
          "routingKey": "media-service.tvshow-genres.unpublished"
        },
        "TvshowPublished": {
          "exchange": "event",
          "routingKey": "media-service.tvshow.published"
        },
        "TvshowUnpublished": {
          "exchange": "event",
          "routingKey": "media-service.tvshow.unpublished"
        },
        "SeasonPublished": {
          "exchange": "event",
          "routingKey": "media-service.season.published"
        },
        "SeasonUnpublished": {
          "exchange": "event",
          "routingKey": "media-service.season.unpublished"
        },
        "EpisodePublished": {
          "exchange": "event",
          "routingKey": "media-service.episode.published"
        },
        "EpisodeUnpublished": {
          "exchange": "event",
          "routingKey": "media-service.episode.unpublished"
        },
        "CollectionPublished": {
          "exchange": "event",
          "routingKey": "media-service.collection.published"
        },
        "CollectionUnpublished": {
          "exchange": "event",
          "routingKey": "media-service.collection.unpublished"
        },
        "DeleteEntity": {
          "exchange": "command",
          "routingKey": "media-service.entity.delete"
        },
        "DeclareImageTypes": {
          "exchange": "command",
          "routingKey": "ax-image-service.*.*.image_types.declare"
        },
        "EntityDeleted": {
          "exchange": "event",
          "routingKey": "media-service.entity.deleted"
        },
        "DeclareCuePointTypes": {
          "exchange": "command",
          "routingKey": "ax-video-service.*.*.cue_point_types.declare"
        }
      },
      "subscriptions": {
        "Inbox": {
          "queue": "media-service:inbox",
          "prefetch": 10,
          "recovery": "deferred_retry",
          "redeliveries": {
            "limit": 5,
            "counter": "mosaic"
          }
        }
      }
    }
  },
  "recovery": {
    "deferred_retry": [
      {
        "strategy": "forward",
        "attempts": 4,
        "publication": "retry_in_10sec",
        "xDeathFix": true
      },
      {
        "strategy": "forward",
        "attempts": 8,
        "publication": "retry_in_30sec",
        "xDeathFix": true
      },
      {
        "strategy": "forward",
        "attempts": 9,
        "publication": "retry_in_30sec",
        "options": {
          "options": {
            "headers": {
              "mosaic_final_error": true
            }
          }
        }
      },
      {
        "strategy": "nack"
      }
    ]
  },
  "redeliveries": {
    "counters": {
      "mosaic": {
        "type": "postgresCounter"
      }
    }
  }
}
