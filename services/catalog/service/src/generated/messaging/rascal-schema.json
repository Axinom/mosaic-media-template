  "recovery": {
    "deferred_retry": [
      {
        "strategy": "forward",
        "attempts": 4,
        "publication": "retry_in_10sec",
        "xDeathFix": true
      },
      {
        "strategy": "forward",
        "attempts": 8,
        "publication": "retry_in_30sec",
        "xDeathFix": true
      },
      {
        "strategy": "forward",
        "attempts": 9,
        "publication": "retry_in_30sec",
        "options": {
          "options": {
            "headers": {
              "mosaic_final_error": true
            }
          }
        }
      },
      {
        "strategy": "nack"
      }
    ]
  },
  "redeliveries": {
    "counters": {
      "mosaic": {
        "type": "postgresCounter"
      }
    }
  }
