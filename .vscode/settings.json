{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": false
  },
  "jestrunner.jestCommand": "yarn util:load-vars jest",
  "files.associations": {
    ".env.*": "dotenv",
    "**/pipelines/**/*.{yaml,yml}": "azure-pipelines, yaml"
  },
  "typescript.tsdk": "./node_modules/typescript/lib",
  "cSpell.words": [
    "Axinom",
    "Axinom's",
    "CMAF", 
    "codegen",
    "Dockerized",
    "Downloadability",
    "fairplay",
    "geolocation",
    "Inflector",
    "loglevel",
    "pilet",
    "pilets",
    "Piral",
    "Piral's",
    "playready",
    "postgraphile",
    "prefetch",
    "RABBITMQ",
    "smil",
    "unassignment",
    "uncommit",
    "unpublish",
    "Unpublishes",
    "unpublishing",
    "upsert",
    "urljoin",
    "vhosts",
    "widevine",
    "zapatos"
  ],
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSaveMode": "file"
  },
  "[yaml]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSaveMode": "file"
  }
}
